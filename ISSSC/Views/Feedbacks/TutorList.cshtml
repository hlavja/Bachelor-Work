@model ISSSC.Models.Meta.StatisticsTutor

@{
    ViewBag.Title = "Feedback";
    ViewBag.ActiveMenuItem = "menu-statistics";
}

@{
    ViewBag.Title = "Feedback";
}
<div class="body-content container-fluid text-center">
    <div class="row content">
        <div class="col-sm-2 sidenav">
        </div>
        <div class="col-sm-8 text-left">
            <h2>Statistiky</h2>
            <table class="table">
                <tr>
                    <th>
                        Tutor
                    </th>
                    <th>
                        Počet hodin
                    </th>
                    <th>
                        MAT
                    </th>
                    <th>
                        INF
                    </th>
                    <th>
                        MECH
                    </th>
                    <th>
                        Odučené hodiny
                    </th>
                </tr>

                @foreach (var item in Model.Tutor)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.IdTutorNavigation.Login)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Lessons)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MathLessons)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.InfLessons)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MechLessons)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LessonsHours)
                        </td>
                    </tr>
                }

            </table>
        </div>
        <div class="col-sm-2 sidenav">

        </div>
    </div>
</div>