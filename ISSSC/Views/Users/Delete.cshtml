@model ISSSC.Models.SscisUser

@{
    ViewBag.Title = @SSCISResources.Resources.DELETE;
    ViewBag.ActiveMenuItem = "menu-users";
}

    <div class="row content">
        <div class="col-sm-2 sidenav">
        </div>
        <div class="col-sm-8 text-left">
            <h2>@SSCISResources.Resources.DELETE_USER</h2>

            <h3>@SSCISResources.Resources.USER_DELETE_CONFIRM</h3>
            <div>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
                <h4>Uživatel</h4>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @SSCISResources.Resources.LOGIN
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Login)
                    </dd>

                    <dt>
                        @SSCISResources.Resources.NAME
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Firstname)
                    </dd>

                    <dt>
                        @SSCISResources.Resources.LASTNAME
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Lastname)
                    </dd>

                    <dt>
                        @SSCISResources.Resources.CREATED
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Created)
                    </dd>

                    <dt>
                        @SSCISResources.Resources.STUDENT_NUMBER
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.StudentNumber)
                    </dd>

                    <dt>
                        @SSCISResources.Resources.ROLE
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.IdRoleNavigation.Role)
                    </dd>
                    <dd>
                        <br />
                        <div>
                            <input type="submit" value="@SSCISResources.Resources.DELETE" class="btn btn-default" />
                        </div>
                    </dd>
                </dl>

        

                    <div class="form-actions no-color">
                        @Html.ActionLink(SSCISResources.Resources.BACK, "Index")
                    </div>
                }
            </div>
        </div>
        <div class="col-sm-2 sidenav">

        </div>
    </div>