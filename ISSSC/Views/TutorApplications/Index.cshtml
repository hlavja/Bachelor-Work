@model IEnumerable<ISSSC.Models.TutorApplication>

@{
    ViewBag.Title = SSCISResources.Resources.TUTOR_APPLICATIONS;
    ViewBag.ActiveMenuItem = "menu-tutors";
}


    <div class="row content">
        <div class="col-sm-2 sidenav">
        </div>

        <div class="col-sm-8 text-left">
            <h1>@SSCISResources.Resources.TUTOR_APPLICATIONS</h1>

            <p>
            </p>
            <table class="table">
                <tr>
                    <th>
                        Datum přihlášky
                    </th>
                    <th>
                        Login uživatele
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.ApplicationDate)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IdUserNavigation.Login)
                        </td>
                        <td>
                            @Html.ActionLink(SSCISResources.Resources.DETAIL, "Details", new { id = item.Id })
                        </td>
                    </tr>
                }

            </table>
        </div>
        <div class="col-sm-2 sidenav">
        </div>
    </div>
